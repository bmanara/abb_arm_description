<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"> 

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_footprint">
    </link>

    <!-- Chassis -->
    <joint name="chassis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="0 0 -0.25"/>
    </joint>
    <link name="chassis">
        <visual>
            <origin xyz="0 0 0.1485" rpy="0 0 0"/>
            <geometry>
                <box size="1.35 0.91 0.297"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0 0 0.15" rpy="0 0 0"/>
            <geometry>
                <box size="1.35 0.91 0.297"/>
            </geometry>
        </collision>
       <xacro:inertial_box mass="244" x="1.35" y="0.91" z="0.297">
            <origin xyz="0 0 0.1485" rpy="0 0 0"/>
        </xacro:inertial_box>
    </link>

    <!-- Left Wheel -->
    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.475 -0.25" rpy="-${pi/2} 0 0"/>
        <axis xyz="0 0 1"/>
        <limit effort="100" velocity="10"/>
    </joint>
    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder length="0.04" radius="0.05"/> 
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.04" radius="0.05"/> 
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="1" length="0.04" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>
    
    <!-- Right Wheel -->
    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.475 -0.25" rpy="${pi/2} 0 0"/>
        <axis xyz="0 0 -1"/>
        <limit effort="100" velocity="10"/>
    </joint>
    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder length="0.04" radius="0.05"/> 
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.04" radius="0.05"/> 
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="1" length="0.04" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_cylinder>
    </link>

    <!-- Caster Wheels -->
    <joint name="top_left_caster_wheel_joint" type="fixed">
        <parent link="chassis"/>
        <child link="top_left_caster_wheel"/>
        <origin xyz="0.615 0.39 0" rpy="0 0 0"/>
    </joint>
    <link name="top_left_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_sphere mass="2" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>
    <gazebo reference="top_left_caster_wheel">
        <mu1 value="0.001" />
        <mu2 value="0.001" />
    </gazebo>

    <joint name="top_right_caster_wheel_joint" type="fixed">
        <parent link="chassis"/>
        <child link="top_right_caster_wheel"/>
        <origin xyz="0.615 -0.39 0" rpy="0 0 0"/>
    </joint>
    <link name="top_right_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_sphere mass="2" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>
    <gazebo reference="top_right_caster_wheel">
        <mu1 value="0.001" />
        <mu2 value="0.001" />
    </gazebo>

    <joint name="bottom_left_caster_wheel_joint" type="fixed">
        <parent link="chassis"/>
        <child link="bottom_left_caster_wheel"/>
        <origin xyz="-0.615 0.39 0" rpy="0 0 0"/>
    </joint>
    <link name="bottom_left_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_sphere mass="2" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>
    <gazebo reference="bottom_left_caster_wheel">
        <mu1 value="0.001" />
        <mu2 value="0.001" />
    </gazebo>

    <joint name="bottom_right_caster_wheel_joint" type="fixed">
        <parent link="chassis"/>
        <child link="bottom_right_caster_wheel"/>
        <origin xyz="-0.615 -0.39 0" rpy="0 0 0"/>
    </joint>
    <link name="bottom_right_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </collision>
        <xacro:inertial_sphere mass="2" radius="0.05">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_sphere>
    </link>
    <gazebo reference="bottom_right_caster_wheel">
        <mu1 value="0.001" />
        <mu2 value="0.001" />
    </gazebo>

</robot>
